{
  "name": "fortune-indexeddb",
  "description": "IndexedDB adapter for Fortune.",
  "version": "1.1.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:fortunejs/fortune-indexeddb.git"
  },
  "scripts": {
